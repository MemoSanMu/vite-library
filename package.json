    "@vitejs/plugin-react": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
